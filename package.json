{
  "name": "discordjs-starter",
  "version": "1.0.0",
  "description": "Starter for Discord Bots using Discord.js",
  "main": "src/index.js",
  "scripts": {
    "start": "per-env",
    "start:development": "nodemon -r dotenv/config .",
    "start:production": "node .",
    "deploy": "per-env",
    "deploy:development": "node -r dotenv/config src/scripts/deploy-commands.js",
    "deploy:production": "node src/scripts/deploy-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iRiceCrispy/discordjs-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iRiceCrispy/discordjs-starter/issues"
  },
  "homepage": "https://github.com/iRiceCrispy/discordjs-starter#readme",
  "dependencies": {
    "discord.js": "^14.8.0",
    "per-env": "^1.0.2"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.21"
  }
}
